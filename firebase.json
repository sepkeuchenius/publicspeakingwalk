{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/captivating-speaking-day-training",
        "destination": "/captivating-speaking-day-training.html"
      },
      {
        "source": "/captivating-speaking-training-workshop",
        "destination": "/captivating-speaking-training-workshop.html"
      },
      {
        "source": "/captivating-speaking-workshop-overview",
        "destination": "/captivating-speaking-workshop-overview.html"
      }
    ]

  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
